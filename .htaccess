RewriteEngine On

# Redirect URLs ending with  to the same URL without the  extension
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\ -f
RewriteRule ^(.+)\$ /$1 [R=301,L]

# Check if the request is not for a real file
RewriteCond %{REQUEST_FILENAME} !-f

# Check if the request is not for a real directory
RewriteCond %{REQUEST_FILENAME} !-d

# Rewrite rule to include slashes in the parameter
RewriteRule ^([a-zA-Z0-9-/]+)$ index?page=$1 [L,QSA]

